{
    "version": "0.3.29",
    "license": "MIT",
    "type": "module",
    "source": "src/sdk/index.ts",
    "main": "./build/sdk/index.cjs",
    "module": "./build/sdk/index.module.js",
    "unpkg": "./build/sdk/index.umd.js",
    "types": "./build/sdk/index.d.ts",
    "umd:main": "./build/sdk/index.umd.js",
    "exports": {
        ".": {
            "types": "./build/sdk/index.d.ts",
            "require": "./build/sdk/index.cjs",
            "import": "./build/sdk/index.module.js"
        },
        "./src/api/tonomy.css": {
            "import": "./src/api/tonomy.css",
            "require": "./src/api/tonomy.css"
        }
    },
    "files": [
        "src",
        "build"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "microbundle watch",
        "build:sdk": "microbundle --compress=false",
        "build:cli": "microbundle --compress=false --target=node -f cjs -i ./src/cli/index.ts -o build/cli/index.js",
        "build": "npm run build:sdk && npm run build:cli",
        "test:unit": "jest --selectProjects \"Unit tests\"",
        "test:setup": "./test-integration/build-and-run-services.sh start",
        "test:integration": "jest --detectOpenHandles --selectProjects \"Integration tests\"",
        "test": "jest --detectOpenHandles",
        "cli": "node ./build/cli/index.cjs",
        "lint": "eslint src test --ext .ts --resolve-plugins-relative-to .",
        "lint:fix": "npm run lint -- --fix",
        "release": "semantic-release  --debug"
    },
    "name": "@tonomy/tonomy-id-sdk",
    "author": "Jack Tanner",
    "devDependencies": {
        "@babel/core": "^7.21.4",
        "@babel/node": "^7.20.7",
        "@babel/preset-env": "^7.21.4",
        "@babel/preset-typescript": "^7.21.4",
        "@semantic-release/changelog": "^6.0.2",
        "@semantic-release/git": "^10.0.1",
        "@types/elliptic": "^6.4.14",
        "@types/jest": "^29.5.0",
        "@types/randombytes": "^2.0.0",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "address": "^1.2.2",
        "babel-jest": "^29.5.0",
        "eslint": "^8.25.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-prettier": "^3.4.1",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "jsdom-url": "^3.0.1",
        "microbundle": "^0.15.1",
        "prettier": "^2.7.1",
        "semantic-release": "^20.1.1",
        "ts-jest": "^29.0.5",
        "tslib": "^2.4.0",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "@consento/sync-randombytes": "^1.0.5",
        "@greymass/eosio": "^0.6.4",
        "@tonomy/antelope-did": "^0.1.5",
        "@tonomy/antelope-did-resolver": "^0.2.2",
        "@tonomy/antelope-ssi-toolkit": "^1.1.0",
        "@tonomy/did-jwt": "^6.10.1",
        "@tonomy/did-jwt-vc": "^3.1.3",
        "@tonomy/did-resolver": "^4.0.4",
        "argon2": "^0.30.3",
        "cross-fetch": "^3.1.5",
        "elliptic": "^6.5.4",
        "socket.io-client": "^4.5.4"
    },
    "publishConfig": {
        "access": "public",
        "registry-url": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Tonomy-Foundation/Tonomy-ID-SDK.git"
    },
    "keywords": [
        "sdk",
        "tonomy"
    ]
}
