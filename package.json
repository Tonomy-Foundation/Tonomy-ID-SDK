{
    "version": "0.3.6",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "npx tsdx watch --transpileOnly --noClean",
        "build": "npx tsdx build",
        "test": "npx tsdx test --config ./test/jest.config.tsdx.js",
        "jest": "jest --config ./test/jest.config.js",
        "test:integration": "tsdx test --detectOpenHandles --config ./test-integration/jest.config.js",
        "bootstrap": "npx ts-node --project ./bootstrap-blockchain/tsconfig.json bootstrap-blockchain/bootstrap.ts",
        "lint": "eslint src test --ext .ts --resolve-plugins-relative-to .",
        "lint:fix": "npm run lint -- --fix",
        "prepare": "npx tsdx build --transpileOnly --noClean",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "name": "@tonomy/tonomy-id-sdk",
    "author": "Jack Tanner",
    "module": "dist/tonomy-id-sdk.esm.js",
    "size-limit": [
        {
            "path": "dist/tonomy-id-sdk.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/tonomy-id-sdk.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^8.0.0",
        "@types/elliptic": "^6.4.14",
        "@types/randombytes": "^2.0.0",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "address": "^1.2.2",
        "argon2": "^0.30.2",
        "eslint": "^8.25.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^3.4.1",
        "husky": "^8.0.1",
        "jest-environment-jsdom": "^24.9.0",
        "jsdom-url": "^3.0.1",
        "prettier": "^2.7.1",
        "size-limit": "^8.0.0",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "tsdx": "^0.14.1",
        "tslib": "^2.4.0",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "@consento/sync-randombytes": "^1.0.5",
        "@greymass/eosio": "^0.6.4",
        "@tonomy/antelope-did": "^0.1.5",
        "@tonomy/antelope-did-resolver": "^0.2.2",
        "@tonomy/antelope-ssi-toolkit": "^1.1.0",
        "@tonomy/did-jwt": "^6.10.1",
        "@tonomy/did-jwt-vc": "^3.1.3",
        "@tonomy/did-resolver": "^4.0.4",
        "cross-fetch": "^3.1.5",
        "elliptic": "^6.5.4",
        "socket.io-client": "^4.5.4"
    },
    "publishConfig": {
        "access": "public",
        "registry-url": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Tonomy-Foundation/Tonomy-ID-SDK.git"
    },
    "keywords": [
        "sdk",
        "tonomy"
    ]
}
