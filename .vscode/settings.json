{
    "eslint.validate": [
        "javascript",
        "typescript"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "cSpell.words": [
        "tonomy"
    ],
    "jestrunner.jestCommand": "npm run test:integration",
    "jest.autoRun": {
        "watch": false
    },
    "jest.rootPath": "./",
    "jest.jestCommandLine": "npm run test"
}