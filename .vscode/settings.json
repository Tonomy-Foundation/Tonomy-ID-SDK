{
    "eslint.validate": [
        "javascript",
        "typescript"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "cSpell.words": [
        "tonomy"
    ],
    "jest.autoRun": {
        "onSave": false,
        "onStartup": false
    },
    "jest.debugMode": false,
    "jest.jestCommandLine": "DEBUG=* yarn run test",
    "jest.testExplorer": {
        "enabled": true
    },
    "markdownlint.config": {
        "default": true,
        "MD033": false
    },
    "git.branchPrefix": "feature/",
    "git.branchProtection": [
        "master",
        "main",
        "testnet",
        "development"
    ],
    "jest.runMode": "on-demand",
    "editor.formatOnSave": false,
}