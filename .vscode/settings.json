{
    "eslint.validate": [
        "javascript",
        "typescript"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "cSpell.words": [
        "tonomy"
    ],
    "jest.autoRun": {
        "onSave": false
    },
    "jest.debugMode": false,
    "jest.jestCommandLine": "npm run test",
    "jest.testExplorer": {
        "enabled": true
    },
}